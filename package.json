{
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/stylesheet -o src/assets/stylesheet/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/stylesheet -o src/assets/stylesheet/css --watch --recursive",
    "start-js": "roadhog server",
    "build-js": "roadhog build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "lint": "eslint --ext .js src test"
  },
  "engines": {
    "install-node": "6.11.1"
  },
  "dependencies": {
    "antd": "^2.13.4",
    "babel-plugin-import": "^1.6.0",
    "babel-runtime": "^6.9.2",
    "braft-editor": "^1.6.1",
    "dva": "^1.2.1",
    "dva-loading": "^1.0.2",
    "dva-model-extend": "^0.1.2",
    "node-sass": "^4.9.3",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "react": "^15.4.0",
    "react-avatar-editor": "^11.0.2",
    "react-dom": "^15.4.0",
    "react-helmet": "^5.2.0",
    "react-image-crop": "^3.0.11",
    "sass-loader": "^7.1.0",
    "source-map": "^0.7.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-dva-hmr": "^0.3.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "expect": "^1.20.2",
    "husky": "^0.12.0",
    "redbox-react": "^1.4.3",
    "roadhog": "^1.2.1"
  }
}
